{
    "name": "Bibliography",
    "shortname": "bibliography",
    "pkg": "de.undercouch.citeproc.output",
    "description": "A generated bibliography consisting of bibliography entries and some formatting parameters",
    "noJsonObject": true,
    "noBuilder": true,
    
    "props": [
        {
            "type": "String[]",
            "name": "entries",
            "required": true
        },
        {
            "type": "String",
            "name": "bibstart",
            "normalizedName": "bibStart"
        },
        {
            "type": "String",
            "name": "bibend",
            "normalizedName": "bibEnd"
        },
        {
            "type": "String[]",
            "name": "entry_ids"
        },
        {
            "type": "Integer",
            "name": "maxoffset",
            "normalizedName": "maxOffset"
        },
        {
            "type": "Integer",
            "name": "entryspacing",
            "normalizedName": "entrySpacing"
        },
        {
            "type": "Integer",
            "name": "linespacing",
            "normalizedName": "lineSpacing"
        },
        {
            "type": "Integer",
            "name": "hangingindent",
            "normalizedName": "hangingIndent"
        },
        {
            "type": "Boolean",
            "name": "done"
        },
        {
            "type": "SecondFieldAlign",
            "name": "second-field-align"
        }
    ],
    
    "additionalMethods":[
        "/**\n* Converts this bibliography to a String that can be inserted into the text\n*/\npublic String makeString() { StringBuilder b = new StringBuilder(); if (getBibStart() != null) b.append(getBibStart()); if (entries != null) { for (String e : entries) b.append(e); } if (getBibEnd() != null) b.append(getBibEnd()); return b.toString(); }"
    ]
}
